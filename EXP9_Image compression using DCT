clc;
close all;
clear all;
 
I = imread(/MATLAB Drive/aditya.jpeg);
I = rgb2gray(I); 
figure(1);
 
subplot(1, 3, 1);
imshow(I);
title('Original Image');
 
I = double(I);
C = dct2(I);
 
subplot(1, 3, 2);
imshow(C, []);
colormap gray; % Specify the colormap
title('DCT of Original Image');
 
O = idct2(C);
O = uint8(O);
 
subplot(1, 3, 3);
imshow(O);
title('IDCT of DCT Image');
